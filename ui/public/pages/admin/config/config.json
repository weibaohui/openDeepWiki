{
  "body": [
    {
      "type": "form",
      "mode": "horizontal",
      "api": "/admin/config/update",
      "initApi": "get:/admin/config/all",
      "title": "配置管理",
      "body": [
        {
          "type": "anchor-nav",
          "links": [
            {
              "title": "AI配置",
              "body": [
                {
                  "type": "fieldSet",
                  "title": "AI配置",
                  "body": [


                    {
                      "name": "api_url",
                      "type": "input-url",
                      "label": "API地址",
                      "value": "",
                      "desc": "大模型的自定义API URL（如需使用内置的API，请留空，并重启应用）"
                    },
                    {
                      "name": "api_model",
                      "type": "input-text",
                      "label": "模型名称",
                      "value": "Qwen/Qwen2.5-7B-Instruct",
                      "desc": "大模型的自定义模型名称（如需使用内置的API，请留空，并重启应用）"
                    },
                    {
                      "name": "api_key",
                      "type": "input-password",
                      "label": "API密钥",
                      "value": "",
                      "desc": "大模型的自定义API Key（如需使用内置的API，请留空，并重启应用）"
                    },
                    {
                      "name": "temperature",
                      "type": "input-number",
                      "precision": 2,
                      "step": 0.01,
                      "label": "模型温度",
                      "value": "0.7",
                      "desc": "temperature,控制大模型输出的随机程度"
                    },
                    {
                      "name": "top_p",
                      "type": "input-number",
                      "precision": 2,
                      "step": 0.01,
                      "label": "核采样",
                      "value": "1",
                      "desc": "top_p,越大词汇量越大，输出越多样"
                    },
                    {
                      "name": "max_history",
                      "type": "input-number",
                      "label": "上下文记忆",
                      "value": "10",
                      "desc": "适当的记忆量有助于提升回答精度"
                    },
                    {
                      "name": "any_select",
                      "type": "switch",
                      "label": "任意选择",
                      "value": false,
                      "desc": "是否开启任意选择，默认开启"
                    }
                  ]
                }
              ]
            },

            {
              "title": "显示设置",
              "body": [
                {
                  "type": "fieldSet",
                  "title": "显示设置",
                  "body": [
                    {
                      "name": "product_name",
                      "type": "input-text",
                      "label": "产品名称",
                      "value": "OpenDeepWiki",
                      "desc": "设置产品显示名称"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}